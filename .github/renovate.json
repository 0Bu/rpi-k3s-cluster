{
  "packageRules": [
    {
      "matchDatasources": [
        "docker"
      ],
      "matchPackageNames": [
        "homebridge/homebridge"
      ],
      "versioning": "loose"
    }
  ],
  "customManagers": [
    {
      "customType": "regex",
      "managerFilePatterns": [
        "^README\\.md$"
      ],
      "matchStrings": [
        "https://raw\\.githubusercontent\\.com/metallb/metallb/v(?<currentValue>[^/]+)/config/manifests/metallb-native\\.yaml"
      ],
      "datasourceTemplate": "github-releases",
      "depNameTemplate": "metallb/metallb",
      "versioningTemplate": "semver"
    },
    {
      "customType": "regex",
      "managerFilePatterns": [
        "/^IPv6\\.md$/"
      ],
      "matchStrings": [
        "export INSTALL_K3S_VERSION=(?<currentValue>.+)"
      ],
      "depNameTemplate": "k3s",
      "datasourceTemplate": "github-releases",
      "packageNameTemplate": "k3s-io/k3s"
    }
  ]
}
